{
    "builds": [{
            "src": "MajorProject/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxlambdaSize": "15mb", "runtime": "python3.9" }
        }],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "MajorProject/wsgi.py"
            }
        ]
}